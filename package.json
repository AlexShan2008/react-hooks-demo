{
  "name": "create-react-next",
  "version": "1.0.0",
  "description": "Create React apps use Next.js with zero config",
  "main": "index.js",
  "repository": "git@github.com:AlexShan2008/create-react-next.git",
  "author": "shantong <shantong@reyun.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "next",
    "build": "next build",
    "build:stats": "webpack --env production -json > stats.json",
    "start": "next start",
    "export": "yarn build && next export",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
    "lint": "lint-staged"
  },
  "pre-commit": [
    "lint"
  ],
  "lint-staged": {
    "*.jsx": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@date-io/moment": "^1.3.8",
    "@material-ui/core": "^4.2.1",
    "@material-ui/icons": "^4.2.1",
    "@material-ui/pickers": "^3.2.0",
    "@material-ui/styles": "^4.2.1",
    "@material-ui/system": "^4.3.1",
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-sass": "^1.0.1",
    "antd": "^3.20.5",
    "axios": "^0.19.0",
    "express": "^4.17.1",
    "mobx": "^5.13.0",
    "mobx-react": "^6.1.1",
    "moment": "^2.24.0",
    "next": "^9.0.2",
    "next-i18next": "^0.50.0",
    "node-sass": "^4.12.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "store": "^2.0.12"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@commitlint/config-conventional": "^8.1.0",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "babel-plugin-import": "^1.12.0",
    "babel-preset-mobx": "^2.0.0",
    "commitlint": "^8.1.0",
    "cz-conventional-changelog": "3.0.2",
    "eslint": "5.3.0",
    "eslint-config-standard": "^13.0.1",
    "eslint-config-standard-react": "^8.0.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.14.2",
    "eslint-plugin-react-hooks": "^1.6.1",
    "eslint-plugin-standard": "^4.0.1",
    "file-loader": "^4.1.0",
    "husky": "^3.0.1",
    "lint-staged": "^9.2.5",
    "pre-commit": "^1.2.2",
    "prettier": "^1.18.2",
    "react-svg-loader": "^3.0.3"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
